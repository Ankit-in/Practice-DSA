
public class Immutablee {

	public static void main(String[] args) {
		String s1 = "Hello"; // String literal
		String s2 = "Hello"; // String literal
		String s3 = s1; // same reference
		String s4 = new String("Hello"); // String object
		String s5 = new String("Hello"); // String object
		
		if(s1 == s1); // true, same pointer
		if(s1 == s2); // true, s1 and s1 share storage in common pool
		if(s1 == s3); // true, s3 is assigned same pointer as s1
		s1.equals(s3); // true, same contents
		if(s1 == s4); // false, different pointers
		s1.equals(s4); // true, same contents
		if(s4 == s5); // false, different pointers in heap
		s4.equals(s5); // true, same contents
	}

}
