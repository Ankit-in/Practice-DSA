
public class Palindrom {
	
	
	public static void main(String[] args) {
		String str="Ama";
		int last=str.length()-1;
		int check=0;
		for(int i=0;i!=last;i++) {
			
			if(str.charAt(i)==str.charAt(last)) {
				last--;
				check++;
			}else {
				System.out.println("no");
				break;
			}
		}
		if(check==str.length()/2) {
			System.out.println("yes");
		}
		
	}

}
