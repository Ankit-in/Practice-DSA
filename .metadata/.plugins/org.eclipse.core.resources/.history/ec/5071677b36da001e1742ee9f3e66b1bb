		char[] str1=str.toCharArray();
		int k=0;
    	for(int i=1;i<str1.length;i++) {
    		if(str1[i]!=str1[k])
    		{
    			str1[k]=str1[i];
    			k++;
    		}
    	}
    	System.out.println(str1);
	}
